net: "./cnn_lstm.prototxt"
base_lr: 0.001000
lr_policy: "step"
gamma: 0.5
stepsize: 20000
display: 6000
test_iter: 0
test_interval: 100000
iter_size: 1
max_iter: 100000
weight_decay: 0.00100
type: "RMSProp"
rms_decay: 0.950000
clip_gradients: 20
average_loss: 200
snapshot: 5000
snapshot_prefix: "snapshot"
snapshot_diff: false
snapshot_after_train: true
solver_mode: CPU
